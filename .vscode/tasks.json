{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "npm",
			"script": "watch",
			"problemMatcher": "$tsc-watch",
			"isBackground": true,
			"presentation": {
				"reveal": "never"
			},
			"group": {
				"kind": "build",
				"isDefault": true
			}
		},
        {
            "label": "lint",
            "type": "npm",
            "script": "lint",
            "group": "test",
            "problemMatcher": []
        },
        {
            "label": "test",
            "type": "npm",
            "script": "test",
            "group": "test",
            "problemMatcher": []
        }
	]
}
